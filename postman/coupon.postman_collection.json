{
  "info": {"name": "Coupon API", "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"},
  "item": [
    {"name": "Create Coupon","request": {"method": "POST","header": [{"key": "Content-Type", "value": "application/json"},{"key": "Authorization", "value": "Bearer <admin_token>"}],"body": {"mode": "raw","raw": "{\n  \"code\": \"DISCOUNT10\",\n  \"discount\": 10,\n  \"expires\": \"2025-12-31T23:59:59.000Z\"\n}"},"url": {"raw": "http://localhost:5000/api/coupons","host": ["http://localhost:5000"],"path": ["api","coupons"]}}},
    {"name": "Get All Coupons","request": {"method": "GET","url": {"raw": "http://localhost:5000/api/coupons","host": ["http://localhost:5000"],"path": ["api","coupons"]}}},
    {"name": "Get Coupon By ID","request": {"method": "GET","url": {"raw": "http://localhost:5000/api/coupons/<couponId>","host": ["http://localhost:5000"],"path": ["api","coupons","<couponId>"]}}},
    {"name": "Update Coupon","request": {"method": "PUT","header": [{"key": "Content-Type", "value": "application/json"},{"key": "Authorization", "value": "Bearer <admin_token>"}],"body": {"mode": "raw","raw": "{\n  \"discount\": 15\n}"},"url": {"raw": "http://localhost:5000/api/coupons/<couponId>","host": ["http://localhost:5000"],"path": ["api","coupons","<couponId>"]}}},
    {"name": "Delete Coupon","request": {"method": "DELETE","header": [{"key": "Authorization", "value": "Bearer <admin_token>"}],"url": {"raw": "http://localhost:5000/api/coupons/<couponId>","host": ["http://localhost:5000"],"path": ["api","coupons","<couponId>"]}}}
  ]
}
